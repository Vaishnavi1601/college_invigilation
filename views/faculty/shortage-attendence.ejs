
 <%- include('../includes/faculty/head.ejs') %>

 <%- include('../includes/faculty/navigation.ejs') %>
 
</header>

<br><br><br>

<section class="container">
  <nav aria-label="breadcrumb">
    <ol class="breadcrumb">
      <li class="breadcrumb-item active" aria-current="page">
        Admin Dashboard / Clear Attendence Exams
      </li>
    </ol>
  </nav>
</section>

<section>
  <div class="container">
    <div class="row">
      <!-- // show attendence of his sem only -->
      <div class="col-md-6 border">
        <br>
        <h4>Shortage Student's Result</h4>
        <% if(shortageAttendence.length > 0) { %>

        <table class="table table-bordered table-striped table-sm">
          <tr>
            <th>BRANCH</th>
            <th>SEM</th>
            <th>Action</th>
          </tr>
          <% for (let sAttendence of shortageAttendence) { %>
          
          <tr>
            <td><%= sAttendence.branch%></td>
            <TD><%= sAttendence.sem%></TD>
            <td>
                <button class="btn btn-primary btn-sm"><a href="/faculty/shortage-attendence/<%= sAttendence._id %>">Download</a>  <i class="fa fa-cloud-download"></i></button>
            </td>
          </tr>
        <% } %>

        </table>
        <% } %>


      </div>
    </div> <!-- // row -->
  </div>
</section>

<br><br><br>


<%- include('../includes/footer.ejs') %>

<%- include('../includes/faculty/end.ejs') %>

